{
    "name": "fenepedia/contao-isotope-product-member-groups",
    "description": "Contao extension to automatically add groups to members when purchasing specific products in Isotope.",
    "type": "contao-bundle",
    "require": {
        "php": ">=8.1",
        "contao/core-bundle": "^4.13 || ^5.0",
        "isotope/isotope-core": "^2.8",
        "symfony/config": "^5.4 || ^6.0",
        "symfony/dependency-injection": "^5.4 || ^6.0",
        "symfony/http-kernel": "^5.4 || ^6.0",
        "symfony/security-core": "^5.4 || ^6.0"
    },
    "require-dev": {
        "contao/easy-coding-standard": "^5.0",
        "contao/rector": "^1.0"
    },
    "license": "LGPL-3.0-or-later",
    "autoload": {
        "psr-4": {
            "Fenepedia\\ContaoIsotopeProductMemberGroups\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Fenepedia",
            "homepage": "https://www.fenepedia.de",
            "role": "Owner"
        },
        {
            "name": "Fritz Michael Gschwantner",
            "email": "fmg@inspiredminds.at",
            "role": "Developer"
        }
    ],
    "homepage": "https://www.fenepedia.de/tools/contao-isotope-erweiterung-produkte-mit-mitgliedergruppen-verkuenpfen",
    "keywords": [
        "isotope",
        "isotope ecommerce",
        "Produkte",
        "Mitgliedergruppen",
        "Digitrale Produkte"
    ],
    "support": {
        "issues": "hhttps://github.com/fenepedia/contao-isotope-product-member-groups/issues",
        "source": "https://github.com/fenepedia/contao-isotope-product-member-groups"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "contao-components/installer": true,
            "php-http/discovery": false,
            "contao-community-alliance/composer-plugin": true
        }
    },
    "extra": {
        "contao-manager-plugin": "Fenepedia\\ContaoIsotopeProductMemberGroups\\ContaoManager\\Plugin"
    }
}
